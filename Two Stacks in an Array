

class twoStacks {
  public:

    twoStacks() {}
    int arr[200];
    int i=0,j=199;
    // Function to push an integer into the stack1.
    void push1(int x) {
        // code here
        if (i<j) {
            arr[i]=x;
            i++;
        }
    }

    // Function to push an integer into the stack2.
    void push2(int x) {
        
        // code here
        if (j>i) {
            arr[j]=x;
            j--;
        }
    }

    // Function to remove an element from top of the stack1.
        
    int pop1() {
        // code here
        if (i==0) {
            return -1;
        }
        else {
            i--;
            return arr[i];
        }
    }

        
    // Function to remove an element from top of the stack2.
    int pop2() {
        // code here
        if (j==199) {
            return -1;
        }
        else {
            j++;
            return arr[j];
        }
    }
};
